{
  "extends": ["airbnb", "airbnb/hooks"],
  "parser": "babel-eslint",
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "import/prefer-default-export": "off",
    "react/jsx-filename-extension": "off",
    "linebreak-style": "off",
    "jsx-a11y/label-has-associated-control": 0
  },
  "env": {
    "browser": true
  }
}